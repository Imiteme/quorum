<%= f.fields_for :hmmer_job do |n| %>
  <p>
    <strong>hmmscan</strong> 
    <%= n.check_box :queue %>
  </p>
  <div id="hmmer" class="toggle">
	  <table class="options">
	    <tr>
	      <td>
	        <%= n.label :expectation %>
	      </td>
	      <td>
	        <%= n.text_field :expectation, :size => 15, :title => "5e-20", 
	          :class => "auto-hint" %>
	      </td>
	      <td>
	        <%= n.label :min_score %>
	      </td>
	      <td>
	        <%= n.text_field :min_score, :size => 15, :title => "0", 
	          :class => "auto-hint" %>
	      </td>
	    </tr>
	  </table>
  </div>
<% end %>
